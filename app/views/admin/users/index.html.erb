<p id="notice"><%= notice %></p>

<h1 class="thin-font">Listing Users</h1>
<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Created at</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.created_at %></td>
        <td><%= link_to 'Show', [:admin,user] %></td>
      </tr>
    <% end %>
  </tbody>
</table>

